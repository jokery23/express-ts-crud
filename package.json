{
  "name": "express-typescript-crud",
  "version": "1.0.0",
  "description": "Express, Typescript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev --respawn src/app.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier --check './src/**/*.ts'",
    "prettier:fix": "prettier --write './src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pre-commit": "npm run prettier:fix && npm run lint:fix",
    "husky:install": "husky install && npx husky add .husky/pre-commit \"npm run pre-commit\"",
    "db:up": "npm run db:migrations && npm run db:seeds",
    "db:down": "npm run db:seeds:down:all && npm run db:migrations:down:all",
    "db:migrations": "node db/migrate up",
    "db:migrations:down": "node db/migrate down",
    "db:migrations:down:all": "node db/migrate down --to 0",
    "db:seeds": "node db/seed up",
    "db:seeds:down": "node db/seed down",
    "db:seeds:down:all": "node db/seed down --to 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-joi-validation": "^5.0.1",
    "joi": "^17.7.0",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "typescript": "^4.5.5",
    "umzug": "^3.2.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-plugin-typescript-enum": "^2.1.0",
    "express-async-handler": "^1.2.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^2.0.0",
    "typedi": "^0.10.0"
  }
}
